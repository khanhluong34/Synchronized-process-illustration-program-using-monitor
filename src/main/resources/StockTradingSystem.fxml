<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="20.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="StockController">
    <children>
        <Label text="Price" />
        <TextField fx:id="priceTextField" editable="false" />
        <Label text="Buy" />
        <TextField fx:id="buyTextField" />
        <Label text="Sell" />
        <TextField fx:id="sellTextField" />
        <Button fx:id="buyButton" text="Buy" />
        <Button fx:id="sellButton" text="Sell" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
